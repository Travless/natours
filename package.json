{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "source": "./public/js/index.js",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "browserslist": "last 2 versions",
  "scripts": {
    "start:dev": "SET NODE_ENV=development&&nodemon server.js",
    "start:prod": "SET NODE_ENV=production&&nodemon server.js",
    "start": "node server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --dist-dir ./public/js/bundled",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Travless/natours.git"
  },
  "author": "Travis White",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Travless/natours/issues"
  },
  "homepage": "https://github.com/Travless/natours#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.30.0",
    "dotenv": "^16.0.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.13.16",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.21",
    "pug": "^3.0.2",
    "sharp": "^0.32.0",
    "slugify": "^1.6.5",
    "validator": "^13.9.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "buffer": "^5.7.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "express": "^4.18.2",
    "ndb": "^1.1.5",
    "parcel": "^2.8.3",
    "prettier": "^2.8.4",
    "process": "^0.11.10"
  }
